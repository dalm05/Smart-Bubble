<!DOCTYPE html>
<html>
<head>
  <title>SmartBubble</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="user_nav", style=" position: absolute; left: 700px; top: 10px">
	<% if authorize_user_signed_in? %>
		Signed in as
		<%= current_authorize_user.username %>
		<%= link_to "Sign out" , destroy_authorize_user_session_path, :method => :delete %>
	<% elsif student_login_signed_in? %>
		Signed in as
		<%= "Student" %>
		<%= link_to "Sign out" , destroy_student_login_session_path, :method => :delete %>
	<% else %>	
	<%end%>
</div>
	<% flash.each do |name, msg| %>
		<%= content_tag :div, msg, :id => "flash_#{name}" %>
	<% end %>
<%= yield %>
</body>
</html>