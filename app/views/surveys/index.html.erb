<h1>Smart-Bubble surveys</h1>
<table>
  <tr>
    <th>Survey id</th>
    <th>Course</th>
    <th>Section</th>
    <th>Instructor</th>        
  </tr>

<% @surveys.each do |survey| %>
  <tr>
    <td><%= survey.id %></td>
    <td><%= survey.course_id %></td>
    <td><%= survey.section_id %></td>
    <td><%= survey.instructor_id %></td>      
    <td><%= link_to 'Show', survey %></td>
    <td><%= link_to 'Edit', edit_survey_path(survey) %></td>
    <td><%= link_to 'Delete', survey, method: :delete, data: 
        { confirm: 'Are you sure you want to delete this survey?' } %></td>
  </tr>
<% end %>
</table>

<br />

<p><%= link_to 'New Survey', new_survey_path %> </p><br />

<%- if devise_mapping.registerable? && controller_name == 'registrations' %>
  <%= link_to "Sign up", new_registration_path(admin) %><br />
<% end -%>