<h2>
<% if resource_name.to_s == 'student_login'%> Student
<% elsif resource_name.to_s == 'authorize_user'%> Authorize Users
<% end %>


Log In</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<%= resource %>
  <% if resource_name.to_s == 'student_login' %>
    <div>
		<%= f.label :Course %><br />   
	 	<%= f.text_field :email %>
  	</div>
  	<div>
  		<%= f.label :Code %><br />
  		<%= f.password_field :password %>
  	</div>
  <% else %>
  	<div>
		<%= f.label :username %><br />   
	 	<%= f.text_field :username %>
  	</div>
  	<div>
  		<%= f.label :password %><br />
  		<%= f.password_field :password %>
  	</div>
  	<div>
  		<%= f.check_box :remember_me %>
  	  	<%= f.label :remember_me %>
  	</div>  
  	    
  <% end -%>

  <div><%= f.submit "Sign in" %></div>
<% end %>

<%= render "devise/shared/links" %>